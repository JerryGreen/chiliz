{
  "name": "chiliz",
  "version": "1.0.2",
  "dependencies": {
    "@just-boris/preact-scripts": "1.1.4",
    "moment": "2.23.0",
    "preact": "10.0.0-alpha.1",
    "preact-compat": "3.18.4",
    "query-string": "5.1.1"
  },
  "devDependencies": {
    "husky": "1.3.1",
    "standard": "12.0.1"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "postbuild": "rm -rf docs && mv build docs",
    "test": "CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "standard"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && CI=true yarn test"
    }
  },
  "standard": {
    "env": [
      "browser",
      "jest"
    ],
    "ignore": [
      "docs"
    ]
  }
}
