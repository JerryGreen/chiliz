{
  "name": "chiliz",
  "version": "1.0.2",
  "dependencies": {
    "ismobilejs": "0.5.1",
    "moment": "2.23.0",
    "preact": "10.0.0-alpha.1",
    "preact-compat": "3.18.4",
    "preact-router": "3.0.0",
    "query-string": "6.4.2"
  },
  "devDependencies": {
    "@types/ismobilejs": "0.4.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.0",
    "babel-core": "6.26.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "husky": "1.3.1",
    "jest": "24.7.0",
    "jsdom": "14.0.0",
    "lynt": "0.5.5",
    "parcel-bundler": "1.9.7",
    "parcel-plugin-typescript": "1.0.0",
    "ts-jest": "24.0.1",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.1"
  },
  "scripts": {
    "start": "parcel --no-autoinstall --port=4000 ./src/index.html",
    "build": "parcel build --out-dir=docs --public-url=./ ./src/index.html",
    "postbuild": "echo chiliz.ru > docs/CNAME",
    "test": "jest",
    "eject": "preact-scripts-ts eject",
    "lint": "tsc --noEmit && tslint 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "alias": {
    "`~/*`": "./src"
  }
}
